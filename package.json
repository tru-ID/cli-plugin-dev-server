{
  "name": "@tru_id/cli-plugin-dev-server",
  "description": "Runs a development server from the tru.ID CLI",
  "version": "0.0.1",
  "author": "Phil Leggetter @leggetter",
  "bugs": "https://github.com/tru-ID/cli-plugin-dev-server/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@tru_id/dev-server": "^0.1.2",
    "fs-extra": "^9.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.6",
    "@types/mocha": "^5",
    "@types/node": "^14.14.22",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "standard-version": "^9.1.0",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock"
  ],
  "homepage": "https://github.com/tru-ID/cli-plugin-dev-server",
  "keywords": [
    "tru-ID-cli-plugins"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tru",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tru-ID/cli-plugin-dev-server",
  "standard-version": {
    "skip": {
      "commit": true,
      "tag": true,
      "bump": true
    }
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "#eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "changelog": "standard-version",
    "publish:canary": "npm publish --tag canary --access public"
  }
}
